{{#each blogs}}
<div class="card border-dark my-3">
  <div class="card-header navbar-dark bg-dark d-flex justify-content-between align-items-center text-light">
    <a href="/editblog/{{id}}">
        <h4>{{title}}</h4>
    </a>
    <p class="mt-3">{{format_date date_created}}</p>
  </div>
</div>
{{/each}}

<button id="newPost" type="button" class="btn btn-dark my-3">+ New Post</button>

<script src="/js/dashboard.js"></script>