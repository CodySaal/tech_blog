{{#each blogs}}
<div class="card border-dark my-3">
  <div class="card-header navbar-dark bg-dark d-flex justify-content-between align-items-center text-light">
    <a href="/blog/{{id}} class="text-decoration-none">
      <h4>{{title}}</h4>
    </a>
    <p class="mt-3">Posted by {{user.username}} on {{format_date date_created}}</p>
  </div>
  <div class="card-body text-dark">
    <p class="card-text">{{content}}</p>
  </div>
</div>
{{/each}}