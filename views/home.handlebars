{{#each blogs}}
<div class="card border-dark mb-3" style="max-width: 18rem;">
  <div class="card-header">
    <h4>{{title}}</h4>
    <p class="mt-3">Posted by {{user.username}} on {{date_created}}</p>
  </div>
  <div class="card-body text-dark">
    <p class="card-text">{{content}}</p>
  </div>
</div>
{{/each}}